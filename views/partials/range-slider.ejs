<div class="slider-container mb-4 mb-0">
	<h6 class="text-uppercase mb-2 text-ex">Accessible Water</h6>
	<div class="range-custom range-blue">
		<input id="slider-water" type="range" min="84" max="6660" value="5400" >
		<div class="count">0 mm</div>
		<div class="fill"></div>
	</div>
</div>

<div class="slider-container mb-4 mb-0">
	<h6 class="text-uppercase mb-2 text-ex">Carbon Level</h6>
	<div class="range-custom range-teal">
		<input id="slider-carbon" type="range" min="20" max="5721" value="3200" >
		<div class="count">0 g</div>
		<div class="fill"></div>
	</div>
</div>

<div class="slider-container">
	<h6 class="text-uppercase mb-2 text-ex">Quality of Soil</h6>
	<div class="range-custom range-red">
		<input id="slider-soil" type="range" min="0" max="3" value="1" >
		<div class="count">Poor</div>
		<div class="fill"></div>
	</div>
</div>